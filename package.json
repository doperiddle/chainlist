{
  "name": "chainlist",
  "version": "1.0.0",
  "private": true,
  "scripts": {https://app.snyk.io/org/doperiddle/project/1d581a33-ad34-47e2-9e7e-581bf17f0683#issue-SNYK-JS-FORMDATA-10841150
import base64
from bitcoinlib.psbt import PSBT

psbt_base64 = ""

# Decode the base64 PSBT
psbt_bytes = base64.b64decode(psbt_base64)
psbt = PSBT.parse(psbt_bytes)

# Check if the PSBT is finalized
if psbt.is_final():
    print("PSBT is finalized. Extracting raw transaction...")
    raw_tx = psbt.transaction.raw_hex()
    print("Raw transaction hex:", raw_tx)
    print("\nYou can broadcast this raw transaction hex using a Bitcoin node or block explorer.")
else:
    print("PSBT is not finalized. It requires more signatures or finalization.")
    print("Please ensure all parties have signed the PSBT before finalizing.")
    "dev": "next dev",
    "build": "./scripts/build.sh",
    "export": "next export && ./scripts/post-export.sh",
    "build2": "./scripts/build.sh && next export",
    "start": "next start",
    "serve": "serve out"
  },
